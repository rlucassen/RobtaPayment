<h2>Nieuwe activiteit</h2>
<div id="form">
  ${Form.FormTag({@action: 'Create', "immediate":'true', "useLabels":'true', "class":'form-horizontal'})}
  <fieldset>
	<%OutputSubView("_activity") %>
	<div class="form-actions">
    ${Form.Submit("Activiteit opslaan", {@class:'btn btn-primary'})}
    <a href="index" class="btn">Annuleren</a>
  </div>
  </fieldset>
${Form.EndFormTag()}
</div>
