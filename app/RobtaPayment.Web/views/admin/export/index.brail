<h1>Exporteren van gegevens</h1>

<form action="enrolmentexport" class="form-horizontal" method="post">
  <br/>
    <div class="control-group">
      ${Form.LabelFor("schoolYear", "Schooljaar", {"class":"control-label"})}
      <div class="controls">
        ${Form.Select("schoolYear", schoolyears, {"value":"id","text":"name"})}
      </div>
    </div>
    <div class="form-actions">
      ${Form.Submit("CSV Export maken", {@class:'btn btn-primary'})}
    </div>
</form>
